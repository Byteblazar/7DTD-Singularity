<Byteblazar>

<append xpath="Sounds">

	<SoundDataNode name="bbxfx_ringingEars"> <AudioSource name="@:Sounds/Prefabs/AudioSource_UI.prefab"/>
		<!-- <AudioClip ClipName="#@modfolder:Resources/sfx.unity3d?tinnitus_fadeout"/> -->
		<AudioClip ClipName="#@modfolder:Resources/sfx.unity3d?tinnitus_loop" Loop="true"/>
		<LocalCrouchVolumeScale value="1"/> <CrouchNoiseScale value="1.0"/> <NoiseScale value="1"/> <MaxVoices value="1"/> <MaxRepeatRate value="1"/> </SoundDataNode>
		
	<SoundDataNode name="bbxfx_loudBang"> 
	<AudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire.prefab"/>
	<!-- <AudioSource name="#@modfolder:Resources/sfx.unity3d?AudioSource_LoudBang.prefab"/> -->
	<NetworkAudioSource name="@:Sounds/Prefabs/AudioSource_WeaponFire_Network.prefab"/>
	<Noise ID="5" noise="200" time="2" heat_map_strength="0.6" heat_map_time="240"/>
		<AudioClip ClipName="#@modfolder:Resources/sfx.unity3d?flashThunderGrenade"/>
		<LowestPitch name="0.95"/>
		<HighestPitch name="1.05"/>
		<LocalCrouchVolumeScale value="1"/> <CrouchNoiseScale value="1"/> <NoiseScale value="1"/> <MaxVoices value="20"/> <MaxVoicesPerEntity value="6"/> <MaxRepeatRate value="0.001"/> </SoundDataNode>
		
	<SoundDataNode name="bbxfx_fastHeartbeat"> <AudioSource name="@:Sounds/Prefabs/AudioSource_PlayerLoop.prefab"/>
		<AudioClip ClipName="#@modfolder:Resources/sfx.unity3d?fastHeartbeat" Loop="true"/>
		<maxVoicesPerEntity value="1"/>
		<LocalCrouchVolumeScale value="1.0"/> <NoiseScale value="1"/> <MaxVoices value="10"/> <MaxRepeatRate value="0.01"/>
		</SoundDataNode>
	
</append>

</Byteblazar>